  + OnEnter() : void
  + OnExit() : void
}

class OpContext {
}

package GeneralStates {

class UninitializedState << (S,#FF7700) State >> {
  + OnEnter() : void
  + OnExit() : void
  __
  <i><color:Gray>Initial state when application starts</color></i>
  <i><color:Gray>Empty implementation - placeholder state</color></i>
